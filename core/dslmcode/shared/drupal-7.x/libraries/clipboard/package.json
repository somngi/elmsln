{
  "name": "clipboard",
  "version": "1.5.5",
  "description": "Modern copy to clipboard. No Flash. Just 2kb",
  "repository": "zenorocha/clipboard.js",
  "license": "MIT",
  "main": "lib/clipboard.js",
  "keywords": [
    "clipboard",
    "copy",
    "cut"
  ],
  "dependencies": {
    "good-listener": "^1.1.4",
    "select": "^1.0.6",
    "tiny-emitter": "^1.0.0"
  },
  "devDependencies": {
    "babel": "5.8.38",
    "babelify": "6.4.0",
    "bannerify": "Vekat/bannerify#feature-option",
    "browserify": "11.2.0",
    "chai": "3.5.0",
    "karma": "0.13.22",
    "karma-browserify": "4.4.2",
    "karma-chai": "0.1.0",
    "karma-mocha": "0.2.2",
    "karma-phantomjs-launcher": "0.2.3",
    "karma-sinon": "1.0.5",
    "mocha": "2.5.3",
    "phantomjs": "1.9.20",
    "phantomjs-polyfill": "0.0.1",
    "sinon": "1.17.7",
    "uglify-js": "2.8.29",
    "watchify": "3.11.0"
  },
  "scripts": {
    "build": "npm run build-debug && npm run build-min",
    "build-debug": "browserify src/clipboard.js -s Clipboard -t [babelify --loose all] -p [bannerify --file .banner ] -o dist/clipboard.js",
    "build-min": "uglifyjs dist/clipboard.js --comments '/!/' -m screw_ie8=true -c screw_ie8=true,unused=false -o dist/clipboard.min.js",
    "build-watch": "watchify src/clipboard.js -s Clipboard -t [babelify --loose all] -o dist/clipboard.js -v",
    "test": "karma start --single-run",
    "prepublish": "babel src --out-dir lib --loose all"
  }
}
